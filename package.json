{
  "name": "form-with-mails",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "vite:dev": "vite",
    "vite:build": "vite build",
    "build": "node ace build --production --tsconfig=./tsconfig.build.json",
    "prebuild": "yarn vite:build",
    "start": "node server.js",
    "test": "node ace test",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.9.5",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "^2.5.1",
    "@tailwindcss/forms": "^0.5.3",
    "@types/proxy-addr": "^2.0.0",
    "@types/source-map-support": "^0.5.6",
    "@vitejs/plugin-react": "^3.0.0",
    "adonis-preset-ts": "^2.1.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "pino-pretty": "^10.0.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.2",
    "typescript": "~4.6",
    "vite": "^4.3.9",
    "youch": "^3.2.3",
    "youch-terminal": "^2.2.0"
  },
  "dependencies": {
    "@91codes/adonis-vite": "^0.3.2",
    "@adonisjs/core": "^5.8.0",
    "@adonisjs/mail": "^8.2.0",
    "@adonisjs/repl": "^3.1.0",
    "@adonisjs/session": "^6.4.0",
    "@adonisjs/view": "^6.2.0",
    "@eidellev/adonis-stardust": "^3.0.0",
    "@eidellev/inertia-adonisjs": "^8.0.1",
    "@hookform/resolvers": "^3.1.1",
    "@inertiajs/progress": "^0.2.7",
    "@inertiajs/react": "^1.0.8",
    "@material-ui/core": "^4.12.4",
    "@microsoft/microsoft-graph-client": "^3.0.5",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "aws-sdk": "^2.1399.0",
    "csv-writer": "^1.6.0",
    "flowbite": "^1.6.6",
    "flowbite-react": "^0.4.7",
    "proxy-addr": "^2.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-headless-notifier": "^0.1.6",
    "react-hook-form": "^7.44.3",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "string-table": "^0.1.5",
    "sweetalert2": "^11.7.12",
    "sweetalert2-react-content": "^5.0.7",
    "zod": "^3.21.4"
  }
}
